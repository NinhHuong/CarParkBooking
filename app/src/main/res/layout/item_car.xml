<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/txtVehicleNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="text here"
        android:layout_marginStart="11dp"
        android:layout_alignBaseline="@+id/btnDelete"
        android:layout_alignBottom="@+id/btnDelete"
        android:layout_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/car_number"
        android:layout_alignBaseline="@+id/txtVehicleNumber"
        android:layout_alignBottom="@+id/txtVehicleNumber"
        android:layout_toEndOf="@+id/imageView4" />

    <Button
        android:id="@+id/btnDelete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="30dp"
        android:width="@dimen/cardview_compat_inset_shadow"
        android:background="@android:drawable/ic_menu_close_clear_cancel"
        android:elevation="0dp"
        android:scaleX=".25"
        android:scaleY=".5" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginStart="24dp"
        app:srcCompat="@drawable/ic_directions_car_black_24dp" />

    <!--<Button-->
        <!--android:id="@+id/btnDelete"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignBottom="@+id/txtVehicleNumber"-->
        <!--android:layout_alignParentTop="true"-->
        <!--android:layout_marginStart="35dp"-->
        <!--android:layout_toEndOf="@+id/txtVehicleNumber"-->
        <!--android:text="Button" />-->

</RelativeLayout>
